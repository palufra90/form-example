---
import { formState } from "./form.store";

let userName = "";
let email = "";
if (Astro.request.method === "POST") {
  try {
    ({ userName, email } = formState.get());
  } catch (error) {
    if (error instanceof Error) {
      console.error(error.message);
    }
  }
}
---

<h4>Form state</h4>
<p>
  name: {userName}
  <br />
  email: {email}
</p>
