{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@api/*": [
        "./src/pages/api/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}